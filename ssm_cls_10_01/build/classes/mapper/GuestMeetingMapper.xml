<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bawei.dao.GuestMeetingDao">
	<insert id="addRelation">
		insert into guest_meeting(mid,gid)
		values(#{mid},#{gid})
	</insert>
	<delete id="deleteByGuestId">
		delete from guest_meeting
		where gid=#{gid}
	</delete>
	
	<insert id="addRelation1">
		insert into guest_meeting(mid,gid) values
		<foreach collection="mids" separator="," item="mid">
			(#{mid},#{gid})
		</foreach>
	</insert>
</mapper>









